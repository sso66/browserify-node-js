{
  "name": "webpack-node.js",
  "version": "1.2.0",
  "description": "Using Node.js, React.jsx and Browserify transpiler for NPM builds",
  "main": "index.js",
  "scripts": {
    "build": "browserify -t [ babelify ] src/index.js -o build/bundle.js",
    "react": "browserify -t [ babelify --presets [ react ] ] src/index-react.js -o build/app.js",
    "test": "jest --coverage",
    "start": "lite-server --bs-config.json",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sso66/webpack-node.js.git"
  },
  "keywords": [
    "nodejs",
    "browserify",
    "webpack",
    "babel",
    "lodash",
    "react",
    "storybook"
  ],
  "author": "Sun Stephen Oo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sso66/webpack-node.js/issues"
  },
  "homepage": "https://github.com/sso66/webpack-node.js#readme",
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@storybook/addon-actions": "^6.5.5",
    "@storybook/addon-essentials": "^6.5.5",
    "@storybook/addon-interactions": "^6.5.5",
    "@storybook/addon-links": "^6.5.5",
    "@storybook/builder-webpack4": "^6.5.5",
    "@storybook/manager-webpack4": "^6.5.5",
    "@storybook/react": "^6.5.5",
    "@storybook/testing-library": "^0.0.11",
    "babel-loader": "^8.2.5",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^17.0.0",
    "jest": "^27.5.1",
    "lite-server": "^2.6.1",
    "lodash": "^4.17.21"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "engines": {
    "*node": "*"
  }
}
